{
  "version": "1.0.0",
  "last_updated": "2026-01-14",
  "description": "Configuration for Response Time ETL filtering rules",
  "filters": {
    "how_reported": {
      "exclude": [
        "Self-Initiated"
      ],
      "description": "How Reported values to exclude from response time calculations"
    },
