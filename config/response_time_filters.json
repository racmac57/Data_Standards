        "Investigations and Follow-Ups",
        "Community Engagement"
      ],
      "description": "CallType Category_Type values to exclude (entire categories)"
    },
    "incidents": {
      "exclude": [
        "Temporary Parking",
        "Traffic Bureau Report",
        "Traffic Detail",
        "Traffic Detail - Road Closure",
        "Traffic Enforcement Detail",
        "Constable - Secured Park Facilities",
        "Patrol Check",
        "Patrol Check - Extra Duty Detail",
        "Shopping Cart",
        "Street Light",
        "TAPS - Business",
        "TAPS - Housing",
        "TAPS - Medical Facility",
        "TAPS - Other",
        "TAPS - Park",
        "TAPS - Parking Garage",
        "TAPS - Religious Facility",
        "TAPS - School",
        "Virtual - Patrol",
        "Weather Alerts",
        "Cleared - Arrest",
        "Cleared-Arrest",
        "Complaint Signed",
        "Assist Own Agency (Backup)",
        "ESU - Response",
        "ESU - Training",
        "PRD Activation",
        "RDF Deployment",
        "RDT Deployment",
        "TAPS - ESU - Business",
        "TAPS - ESU - Medical Facility",
        "TAPS - ESU - Park",
        "TAPS - ESU - Parking Garage",
        "TAPS - ESU - Religious Facility",
        "TAPS - ESU - School",
